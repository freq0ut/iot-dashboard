# .env.example

# PostgreSQL Database
DATABASE_URL=postgresql+asyncpg://iot_user:securepassword@localhost/iot_backend

# AWS Credentials (for DynamoDB)
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key
